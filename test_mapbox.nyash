// MapBoxが正しく動作するかテスト

print("=== MapBox Test ===")

local map
map = new MapBox()

// 値を設定
map.set("name", "Alice")
map.set("age", 25)

// 値を取得
print("Name: " + map.get("name"))
print("Age: " + map.get("age"))

// 存在しないキー
local value
value = map.get("unknown")
if (value == null) {
    print("Unknown key returns null: OK")
}

print("Done!")
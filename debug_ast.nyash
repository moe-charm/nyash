// AST解析用シンプルテスト

box TestBox {
    init { nodeId }
    
    testMethod(arg1, arg2, arg3) {
        print("Called: " + arg1 + ", " + arg2 + ", " + arg3)
    }
    
    callSelf() {
        me.testMethod("first", "second", me.nodeId)
    }
}

local testObj
testObj = new TestBox()
testObj.nodeId = "test"
testObj.callSelf()
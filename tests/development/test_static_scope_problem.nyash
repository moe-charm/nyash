static box TestHelper {
    test_method() {
        return "TestHelper method called successfully"
    }
    
    calculate(x, y) {
        return x + y
    }
}

// ãƒ¡ã‚¤ãƒ³ã®static boxï¼ˆå‘¼ã³å‡ºã—å…ƒï¼‰
static box Main {
    init { console }
    
    main() {
        me.console = new ConsoleBox()
        me.console.log("ğŸš¨ Testing static box â†’ static box scope resolution")
        
        // Test 1: ç›´æ¥çš„ãªé™çš„ãƒ¡ã‚½ãƒƒãƒ‰å‘¼ã³å‡ºã—
        me.console.log("Test 1: Direct static method call")

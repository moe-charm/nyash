// è¦ªBox
box Animal {
    init { name, species }
    
    pack(animalName, animalSpecies) {
        me.name = animalName
        me.species = animalSpecies
        print("ğŸ¾ Animal pack: " + animalName + " (" + animalSpecies + ")")
    }
    
    speak() {
        return me.name + " makes a sound"
    }
}

// å­Box - packæ§‹æ–‡ã§ãƒ‡ãƒªã‚²ãƒ¼ã‚·ãƒ§ãƒ³
box Dog from Animal {
    init { breed }
    
    pack(dogName, dogBreed) {
        from Animal.pack(dogName, "Dog")
        me.breed = dogBreed
        print("ğŸ• Dog pack: " + dogName + " (breed: " + dogBreed + ")")
    }
    
    override speak() {
        return me.name + " barks!"
    }
    
    getBreed() {
        return me.breed
    }
}


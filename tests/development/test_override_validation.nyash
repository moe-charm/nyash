// 1. âœ… æ­£å¸¸ãªoverrideï¼ˆåŸºæœ¬ãƒ†ã‚¹ãƒˆï¼‰
box Animal {
    init { name }
    
    pack(animalName) {
        me.name = animalName
    }
    
    speak() {
        return me.name + " makes a sound"
    }
    
    move() {
        return me.name + " moves"
    }
}

box Dog from Animal {
    init { breed }
    
    pack(dogName, dogBreed) {
        from Animal.pack(dogName)
        me.breed = dogBreed
    }
    
    // âœ… æ­£å¸¸ãªoverride - speakãƒ¡ã‚½ãƒƒãƒ‰ã¯è¦ªã«å­˜åœ¨
    override speak() {
        return me.name + " (dog) barks: Woof!"
    }
    
    // âœ… æ­£å¸¸ãªoverride - moveãƒ¡ã‚½ãƒƒãƒ‰ã‚‚è¦ªã«å­˜åœ¨
    override move() {
        return me.name + " runs fast"
    }
}


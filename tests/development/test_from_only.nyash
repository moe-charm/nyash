
box SimpleChild : P2PBox {
    constructor(nodeId, world) {
        from P2PBox.constructor(nodeId, world)
    }
    
    test() {
        print("Testing from call")
        return from P2PBox.send("test", "data", "target")
    }
}

world = new IntentBox()  
child = new SimpleChild("test", world)
result = child.test()
print("Result: " + result)


box Dog from Animal {
    init { breed }
    
    init(dogName, dogBreed) {
        from Animal.init(dogName, "Dog")
        me.breed = dogBreed
        print("ğŸ• Dog init: " + dogName + " (breed: " + dogBreed + ")")
    }
    
    override speak() {
        return me.name + " barks!"
    }
    
    getBreed() {
        return me.breed
    }
}


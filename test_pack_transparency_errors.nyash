# сЃєсѓ╣сЃѕ1: т╝ЋТЋ░СИЇСИђУЄ┤сѓесЃЕсЃ╝
box BadStringBox from StringBox {
    init { prefix }
    
    birth(content, extra) {
        # StringBoxсЂ»1т╝ЋТЋ░сЂасЂї2сЂцТИАсЂЎсЂесѓесЃЕсЃ╝
        from StringBox(content, extra)
        me.prefix = "test"
    }
}

print("сѓесЃЕсЃ╝сЃєсѓ╣сЃѕ1: т╝ЋТЋ░СИЇСИђУЄ┤")

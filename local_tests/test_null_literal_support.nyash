    main() {
        me.console = new ConsoleBox()
        me.console.log("üß™ Testing null literal support")
        
        // Test 1: Basic null assignment
        local null_value = null
        me.console.log("Null value: " + null_value)
        
        // Test 2: Null comparison
        if null_value == null {
            me.console.log("‚úÖ Null comparison works")
        } else {
            me.console.log("‚ùå Null comparison failed")
        }
        
        // Test 3: Null vs non-null

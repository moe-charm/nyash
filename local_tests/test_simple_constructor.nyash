// テスト: シンプルコンストラクタ

box SimpleBox {
    init { value }
}

static box Main {
    init { console }
    
    main() {
        me.console = new ConsoleBox()
        me.console.log("🧪 シンプルコンストラクタテスト")
        
        // これでSimpleBoxコンストラクタが自動で呼ばれるはず
        local box1 = new SimpleBox("test")

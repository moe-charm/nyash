// 🧪 ArrayBoxの簡単なテスト

print("=== ArrayBox Simple Test ===")
local arr
arr = new ArrayBox()

// 基本的な操作
arr.push("Hello")
arr.push("World")
print("Length: " + arr.length())
print("Get 0: " + arr.get(0))
print("Get 1: " + arr.get(1))

// pop
local item
item = arr.pop()
print("Popped: " + item)
print("Length after pop: " + arr.length())

print("Test completed!")